    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "nodemon app.js",
    "docker-build": "docker build -t forbsey/api.tutorialedge.net:latest .",
    "docker-run": "docker run -d -p 3000:3000 api.tutorialedge.net",
    "docker-push": "docker push forbsey/api.tutorialedge.net",
    "nodes": "kubectl --kubeconfig=kube/cluster-config.yaml get nodes",
    "kube-deployment": "kubectl --kubeconfig=kube/cluster-config.yaml apply -f kube/deployment.yml",
    "kube-service": "kubectl --kubeconfig=kube/cluster-config.yaml apply -f kube/service.yml",
    "kube-update": "kubectl --kubeconfig=kube/cluster-config.yaml rolling-update api.tutorialedge.net -f kube/service.yml"
  },
